    "husky": "^4.3.7",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.12.2",
    "webpack-cli": "3",
    "webpack-dev-server": "^3.11.1",
    "wrangler": "^0.0.2"
