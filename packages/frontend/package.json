{
  "name": "irwin-studio-frontend",
  "version": "0.0.12",
  "description": "Frontend for Irwin Studio",
  "main": "index.js",
  "scripts": {
    "test": "yarn test:types && yarn test:lint && yarn test:unit",
    "test:lint": "eslint \"./src/**/*.{js,ts,tsx}\" --quiet --fix",
    "test:types": "tsc --noEmit",
    "test:unit": "jest",
    "clean": "rm -r ./build",
    "start:cosmos": "cosmos --mode=development --cosmos",
    "start": "webpack-dev-server --mode=development",
    "start:worker": "yarn build && wrangler dev",
    "build": "webpack --mode=production --progress"
  },
  "author": "Lauchlan Irwin <lauchlan.irwin@gmail.com>",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@adamdickinson/react-service": "^2.0.0",
    "firebase": "^8.8.0",
    "konva": "^7.2.5",
    "lodash": "^4.17.20",
    "mdi-react": "^7.5.0",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-konva": "^17.0.2-0",
    "react-particles-js": "^3.4.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "rxjs": "^7.1.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@cloudflare/wrangler": "^1.17.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "copy-webpack-plugin": "^7.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.7",
    "jest": "^27.0.4",
    "prettier": "^2.2.1",
    "react-cosmos": "^5.6.2",
    "source-map-loader": "^2.0.1",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^8.0.14",
    "typescript": "^4.3.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.12.2",
    "webpack-cli": "3",
    "webpack-dev-server": "^3.11.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
