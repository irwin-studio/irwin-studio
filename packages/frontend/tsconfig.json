{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "declaration": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es2018"
  },
}
